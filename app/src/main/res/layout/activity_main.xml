                app:layout_constraintStart_toEndOf="@+id/osc_1_type"
                app:layout_constraintTop_toTopOf="@+id/osc_1_attack" />

            <com.example.sensorinstrument.mainActivity.RotaryKnobView
                android:id="@+id/osc_1_amp"
                class="com.example.sensorinstrument.mainActivity.RotaryKnobView"
                android:layout_width="@dimen/knob_width"
                android:layout_height="@dimen/knob_height"
                android:layout_marginStart="48dp"
                android:layout_marginTop="16dp"
                app:initialValue="50"
                app:knobDrawable="@drawable/ic_rotary_knob"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/osc_1_octave"
                app:maxValue="100"
                app:minValue="0" />

            <TextView
                android:id="@+id/textAmplitude_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="42dp"
                android:layout_marginTop="4dp"
                android:text="@string/amplitude"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/osc_1_amp" />

            <TextView
