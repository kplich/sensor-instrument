<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rootLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="@dimen/defaultMargin"
    tools:context=".mainActivity.MainActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/oscillators_chips"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.00">


        <com.google.android.material.chip.ChipGroup
            android:id="@+id/osc_chips"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:chipSpacingHorizontal="0dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:singleSelection="true">

            <com.google.android.material.chip.Chip
                android:id="@+id/osc_1_chip"
                style="@style/Widget.MaterialComponents.Chip.Choice"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="16dp"
                android:text="@string/osc1"
                android:textAlignment="center"
                android:theme="@style/Theme.MaterialComponents"
                tools:text="@string/osc1" />

            <com.google.android.material.chip.Chip
                android:id="@+id/osc_2_chip"
                style="@style/Widget.MaterialComponents.Chip.Choice"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="16dp"
                android:text="@string/osc2"
                android:theme="@style/Theme.MaterialComponents"
                tools:text="@string/osc2" />

            <com.google.android.material.chip.Chip
                android:id="@+id/osc_3_chip"
                style="@style/Widget.MaterialComponents.Chip.Choice"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="16dp"
                android:text="@string/osc3"
                android:theme="@style/Theme.MaterialComponents"
                tools:text="@string/osc3" />
        </com.google.android.material.chip.ChipGroup>
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/oscillators"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/oscillators_chips">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/osc_1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <SeekBar
                android:id="@+id/osc_1_release"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="24dp"
                android:progress="50"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/textRelease_1"
                app:layout_constraintTop_toBottomOf="@+id/osc_1_sustain" />

            <TextView
                android:id="@+id/textSustain_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="22dp"
                android:text="@string/sustain"
                app:layout_constraintStart_toStartOf="@+id/textDecay_1"
                app:layout_constraintTop_toBottomOf="@+id/textDecay_1" />

            <TextView
                android:id="@+id/textRelease_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="23dp"
                android:text="@string/release"
                app:layout_constraintStart_toEndOf="@+id/textSustain_1"
                app:layout_constraintStart_toStartOf="@+id/textSustain_1"
                app:layout_constraintTop_toBottomOf="@+id/textSustain_1" />

            <SeekBar
                android:id="@+id/osc_1_sustain"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="24dp"
                android:progress="50"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/textSustain_1"
                app:layout_constraintTop_toBottomOf="@+id/osc_1_decay" />

            <TextView
                android:id="@+id/textDecay_1"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="23dp"
                android:text="@string/decay"
                app:layout_constraintStart_toEndOf="@+id/osc_1_type"
                app:layout_constraintTop_toBottomOf="@+id/textAttack_1" />

            <SeekBar
                android:id="@+id/osc_1_decay"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="24dp"
                android:progress="50"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/textDecay_1"
                app:layout_constraintTop_toBottomOf="@+id/osc_1_attack" />

            <com.google.android.material.chip.ChipGroup
                android:id="@+id/osc_1_type"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:chipSpacingHorizontal="0dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:singleSelection="true">

                <com.google.android.material.chip.Chip
                    android:id="@+id/osc_1_type_sine"
                    style="@style/Widget.MaterialComponents.Chip.Choice"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/sine"
                    android:theme="@style/Theme.MaterialComponents"
                    tools:text="@string/sine" />

                <com.google.android.material.chip.Chip
                    android:id="@+id/osc_1_type_saw"
                    style="@style/Widget.MaterialComponents.Chip.Choice"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/sawtooth"
                    android:theme="@style/Theme.MaterialComponents"
                    tools:text="@string/sawtooth" />

                <com.google.android.material.chip.Chip
                    android:id="@+id/osc_1_type_square"
                    style="@style/Widget.MaterialComponents.Chip.Choice"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/square"
                    android:theme="@style/Theme.MaterialComponents"
                    tools:text="@string/square" />
            </com.google.android.material.chip.ChipGroup>

            <com.google.android.material.chip.ChipGroup
                android:id="@+id/osc_1_octave"
