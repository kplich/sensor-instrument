                app:layout_constraintEnd_toStartOf="@+id/osc_1_amp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/osc_1_octave" />

        </androidx.constraintlayout.widget.ConstraintLayout>

