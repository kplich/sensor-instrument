                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/octave_minus_1"
                    android:theme="@style/Theme.MaterialComponents"
                    tools:text="@string/octave_minus_1" />

                <com.google.android.material.chip.Chip
                    android:id="@+id/osc_1_octave_0"
                    style="@style/Widget.MaterialComponents.Chip.Choice"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/octave_0"
                    android:theme="@style/Theme.MaterialComponents"
                    tools:text="@string/octave_0" />

                <com.google.android.material.chip.Chip
                    android:id="@+id/osc_1_octave_1"
                    style="@style/Widget.MaterialComponents.Chip.Choice"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/octave_plus_1"
                    android:theme="@style/Theme.MaterialComponents"
                    tools:text="@string/octave_plus_1" />
            </com.google.android.material.chip.ChipGroup>

            <SeekBar
                android:id="@+id/osc_1_attack"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:progress="50"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/textAttack_1"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
